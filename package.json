{
	"name": "@storysdk/react-native",
	"version": "1.9.0",
	"description": "React Native components for StorySDK",
	"main": "dist/index.js",
	"module": "dist/index.esm.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "rollup -c",
		"dev": "rollup -w -c",
